{
  "cli": {
    "version": ">= 14.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "ios-simulator": {
    "extends": "development",
    "ios": {
      "simulator": true
    },
    "env": {
        "NPM_CONFIG_FORCE": "true"
      }
  }
  },
  
  "submit": {
    "production": {}
  }
}
